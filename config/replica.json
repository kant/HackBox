{
  "db": {
    "client": "mysql",
    "connection": {
      "database": "$DB_CONNECTION_DATABASE_REPLICA",
      "host": "$DB_CONNECTION_HOST_REPLICA",
      "user": "$DB_CONNECTION_USER_REPLICA",
      "password": "$DB_CONNECTION_PASSWORD_REPLICA",
      "port": "$DB_CONNECTION_PORT",
      "multipleStatements": true
    },
    "pool": {
      "min": 0,
      "max": 100
    },
    "migrations": {
      "tableName": "knex_migrations"
    }
  },
  "logEvents": {
    "log": "*",
    "response": "*",
    "error": "*"
  },
  "serverDebug": false,
  "https": true
}
