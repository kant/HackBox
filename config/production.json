{
  "db": {
    "client": "mysql",
    "connection": {
      "database": "test",
      "host": "$DB_CONNECTION_HOST",
      "user": "$DB_CONNECTION_USER",
      "password": "$DB_CONNECTION_PASSWORD"
    },
    "pool": {
      "min": 4,
      "max": 25
    },
    "migrations": {
      "tableName": "knex_migrations"
    }
  },
  "logEvents": {
    "log": "*",
    "response": "*",
    "error": "*"
  },
  "serverDebug": false,
  "https": true
}